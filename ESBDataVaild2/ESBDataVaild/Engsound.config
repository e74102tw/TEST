				</gateway>
				<timers>
					<timer name="Gateway.Connect" duration="10000"/>
					<timer name="Gateway.Shutdown" duration="60000" interval="1000"/>
				</timers>
			</runtimeEnvironment>
		</runtimeEnvironments>
	</EngsoundSettings>
</configuration>